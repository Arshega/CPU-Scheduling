<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.pc.cpuscheduling.MainActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:textAlignment="center"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="3dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/add"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="9dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="7dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="3dp" />

    <EditText
        android:id="@+id/ETBurstTime4"
        android:layout_width="0dp"
        android:layout_height="42dp"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:hint="P4 Burst Time"
        android:inputType="number"
        android:textSize="14sp"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="@+id/ETBurstTime3"
        app:layout_constraintRight_toRightOf="@+id/ETBurstTime3"
        app:layout_constraintTop_toBottomOf="@+id/ETBurstTime3"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <EditText
        android:id="@+id/ETBurstTime3"
        android:layout_width="0dp"
        android:layout_height="42dp"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:hint="P3 Burst Time"
        android:inputType="number"
        android:textSize="14sp"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="@+id/ETBurstTime2"
        app:layout_constraintRight_toRightOf="@+id/ETBurstTime2"
        app:layout_constraintTop_toBottomOf="@+id/ETBurstTime2"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <EditText
        android:id="@+id/ETBurstTime2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="143dp"
        android:ems="10"
        android:hint="P2 Burst Time"
        android:inputType="number"
        android:textSize="14sp"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="@+id/ETBurstTime1"
        app:layout_constraintRight_toRightOf="@+id/ETBurstTime1"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <EditText
        android:id="@+id/ETBurstTime5"
        android:layout_width="0dp"
        android:layout_height="42dp"
        android:layout_marginTop="9dp"
        android:ems="10"
        android:hint="P5 Burst Time"
        android:inputType="number"
        android:textSize="14sp"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="@+id/ETBurstTime4"
        app:layout_constraintRight_toRightOf="@+id/ETBurstTime4"
        app:layout_constraintTop_toBottomOf="@+id/ETBurstTime4"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <EditText
        android:id="@+id/ETprocess"
        android:layout_width="75dp"
        android:layout_height="0dp"
        android:layout_marginEnd="11dp"
        android:ems="10"
        android:hint="(3-5)"
        android:inputType="number"
        android:textAlignment="center"
        android:textSize="14sp"
        app:layout_constraintRight_toLeftOf="@+id/add"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/add"
        android:layout_marginTop="7dp"
        app:layout_constraintTop_toBottomOf="@+id/spinner2"
        android:layout_marginBottom="10dp"
        android:layout_marginRight="11dp" />

    <EditText
        android:id="@+id/ETBurstTime1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="9dp"
        android:layout_marginEnd="10dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="10dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:hint="P1 Burst Time"
        android:inputType="number"
        android:textSize="14sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/ETBurstTime2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/ETprocess"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Number of Processes"
        android:textSize="18sp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintRight_toLeftOf="@+id/ETprocess"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/ETBurstTime1"
        android:layout_marginTop="12dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ETprocess"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="24dp" />

    <Button
        android:id="@+id/add"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:text="ADD"
        android:layout_marginEnd="3dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/ETBurstTime2"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="48dp"
        android:layout_marginBottom="47dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="3dp" />

    <Button
        android:id="@+id/compute"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="3dp"
        android:layout_marginTop="199dp"
        android:onClick="Compute"
        android:text="Compute"
        android:textSize="12sp"
        android:visibility="invisible"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="134dp"
        android:layout_height="34dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="361dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="133dp"
        android:layout_height="36dp"
        android:textSize="36sp"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="361dp" />

</android.support.constraint.ConstraintLayout>
